      #page-wrapper
        .container-fluid
          // Page Heading
          .row
            // Flash Messages
            if success && success.length > 0
              .alert.alert-success.alert-dismissible.fade-in(role="alert")
                button.close(type="button" data-dismiss="alert" aria-label="Close")
                  span(aria-hidden="true") ×
                p= success
            .col-lg-12
              h1.page-header
                | Dashboard 
                small Statistics Overview
                .col-lg-2.pull-right
                  if running
                    input(id='auto-switch' type="checkbox" name="my-checkbox" data-on-text='ONLINE' data-off-text='OFFLINE' checked data-label-text='Auto')
                  else
                    input(id='auto-switch' type="checkbox" name="my-checkbox" data-on-text='ONLINE' data-off-text='OFFLINE' data-label-text='Auto')
                .col-lg-3.pull-right
                  if manualRunning
                    input(id='manual-switch' type="checkbox" name="my-checkbox" data-on-text='ONLINE' data-off-text='OFFLINE' checked data-label-text='Manual')
                  else
                    input(id='manual-switch' type="checkbox" name="my-checkbox" data-on-text='ONLINE' data-off-text='OFFLINE' data-label-text='Manual')
              ol.breadcrumb
                li.active
                  i.fa.fa-dashboard
                  |  dashboard
          // /.row
          .row
            .col-lg-3.col-md-6
              .panel.panel-primary
                .panel-heading
                  .row
                    .col-xs-3
                      i.fa.fa-user.fa-5x
                    .col-xs-9.text-right
                      .huge #{userCount.count}
