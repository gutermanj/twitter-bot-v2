    // Modal
#show-accounts-modal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') ×
        h4#myModalLabel.modal-title Active Accounts
      .modal-body
        table.table.table-striped
          thead
            tr
              th ID
              th Username
              th Email
              th Completed Trades / Cycle
          tbody.js-accounts

      .modal-footer
        button.btn.btn-default(type='button', data-dismiss='modal') Close



    // Modal
#show-records-modal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel')
  .modal-dialog(role='document')
    .modal-content
